    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:prod": "ng build --prod",
        "update": "ng update",
        "update:cli": "ng update @angular/cli",
        "update:core": "ng update @angular/core",
        "fix-lint": "tslint 'src/**/*.ts' --project ./tsconfig.json --fix",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.ts": [
            "prettier --single-quote --trailing-comma all --print-width 100 --arrow-parens always --tab-width 4 --write",
            "tslint",
            "git add"
        ]
    },
    "private": true,
