        "@angular/forms": "~7.2.0",
        "@angular/http": "^7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "bootstrap": "~4.2.1",
        "core-js": "^2.5.4",
        "imagesloaded": "^4.1.4",
        "isotope-layout": "^3.0.6",
        "masonry-layout": "^4.2.2",
        "material-design-lite": "^1.3.0",
        "ngx-isotopee": "^0.1.11",
